<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Sections">
	<!-- Config for retrying Tests that fail-->
	<listeners>
		<listener class-name="ca.IRM.selenium.config.TestRetryListener" />
	</listeners>
	
	<test thread-count="5" name="Functional Testing">
		<!-- Specify which tests you want to run -->
		<groups>
			<run>
				<include name="testing" />
			</run>
		</groups>
		
		<classes>
			<class name="ca.IRM.selenium.Sections.InvolvedSection" />
			<class name="ca.IRM.selenium.Sections.InvolvedSection2" />
			<class name="ca.IRM.selenium.Sections.InvolvedSection3" />
			<class name="ca.IRM.selenium.Sections.InvolvedSection4" />
			<class name="ca.IRM.selenium.Sections.InvolvedSection5" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
Suite -->
